{
  "name": "femmecubator-ui-repo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.20.0",
    "lodash": "^4.17.20",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-ga": "^3.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "universal-cookie": "^4.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:local:backend": "cd ../femmecubator-api-repo && npm run start-debug",
    "start:local:frontend": "cd ../femmecubator-ui-repo && npm start",
    "start-local": "run-p start:local:* -- --watch",
    "start:mock": "REACT_APP_MOCK_API_TRUE=true npm start",
    "build": "react-scripts build",
    "test": "REACT_APP_MOCK_API_TRUE=true react-scripts test",
    "test:coverage": "REACT_APP_MOCK_API_TRUE=true react-scripts test --env=jsdom --watchAll=false --coverage --passWithNoTests",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.8",
    "dot-env": "0.0.1",
    "miragejs": "^0.1.41",
    "npm-run-all": "^4.1.5"
  },
  "proxy": "http://local.femmecubator.com:3001",
  "prettier": {
    "singleQuote": true
  }
}
