{
  "trailingComma": "es5",
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "printWidth": 100,
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "tabWidth": 4,
        "singleQuote": false
      }
    }
  ]
}
